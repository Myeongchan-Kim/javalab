package maze.playerSet;

import maze.Position;

public class PosStack {
	public static final int DEFAULT_STACK_SIZE = 10;
	PosWithNext stack[];
	
	public PosStack (){
		stack = new PosWithNext[DEFAULT_STACK_SIZE];
	}
	public PosWithNext top(){
		if(this.IsEmpty() == true) return null;
		else return stack[stack.length-1];
	}
	
	public void push(Position pos, Position next){
		return;
	}
	
	public PosWithNext pop(){
		return null;
	}
	
	public boolean IsEmpty(){
		if(stack.length < 1) return true;
		else return false;
	}

}
