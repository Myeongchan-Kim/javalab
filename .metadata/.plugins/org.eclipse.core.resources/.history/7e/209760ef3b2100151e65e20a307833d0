package first;

public class ManTest {

	public static void main(String[] args) {

		// lesson 1;
		
		Man leeMan = new Man();
		leeMan.height = 180;
		leeMan.setName("이승기");
		
		Man kimMan = new Man();
		kimMan.setName("김수현");
	
		System.out.println(leeMan.getName());
		System.out.println(kimMan.getName());
		
		// lesson 2
		Point p1 = new Point();
		Point p2 = new Point();
		
		p1.x = 10;
		p2.x = 20;
		p2 = p1;
		
		System.out.println(p1.x);
		System.out.println(p2.x);
		
		
	}

}
