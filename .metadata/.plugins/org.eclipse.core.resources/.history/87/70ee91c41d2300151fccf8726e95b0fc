package birthday;

public class BirthDay {
	private int year;
	private int month;
	private int day;
	
	public BirthDay (int year, int month, int day){
		this.year = year;
		this.month = month;
		this.day = day;
	}
	
	public boolean isValidDate(){
		int year = this.year;
		int month = this.month;
		int day = this.day;
	}
	

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}

	public int getMonth() {
		return month;
	}

	public void setMonth(int month) {
		this.month = month;
	}

	public int getDay() {
		return day;
	}

	public void setDay(int day) {
		this.day = day;
	}
}