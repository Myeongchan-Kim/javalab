package strategy;

public class TimeTable {
	final static int table[][] = {
		{	0,	16,	22,	44,	29,	20,	41},
		{	16,	0,	28,	31,	45,	36,	49},
		{	22,	28,	0,	32,	23,	42,	53,},
		{	44,	31,	32,	0,	15,	43,	18,},
		{	29,	45,	23,	15,	0,	35,	12,},
		{	20,	36,	42,	43,	35,	0,	25,},
		{	41,	49,	35,	18,	12,	25,	0,}
	};
	
	public static int getTripTime(String departure , String destination){
		int i = getNum(destination);
		int j = getNum(departure);
		if( i< 0 || j < 0)
			return 0;
		return table[i][j];
	}

	private static int getNum(String location) {
		switch(location){
		case "Seoul":
			return 0;
		case "Chuncheon":
			return 1;
		case "WonJu":
			return 2;
		case "Gyeongju":
			return 3;
		case "Deajeon":
			return 4;
		case "Ansan":
			return 5;
		case "Gwangju":
			return 6;
		}
		return -1;
	}

}
