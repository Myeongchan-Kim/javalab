package scoreSystem;

public class Subject {
	String name;
	static boolean isPF;
	int score;
	char grade;
	
	public Subject (String name){
		name = this.name;
		isPF = false;
	}
	
	public Subject(String name, boolean pf){
		name = this.name;
		Subject.isPF = pf;
	}
	
	public Subject(String name, int score){
		name = this.name;
		this.score = score;
	}
	
	public String toString(){
		return this.name;
	}
}