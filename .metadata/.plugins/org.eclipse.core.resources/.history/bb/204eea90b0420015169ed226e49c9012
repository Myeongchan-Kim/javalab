package maze;

public class Position {
	public int x;
	public int y;
	boolean wall;
	boolean visited;
	public Position(){
		this(0,0);
	}
	
	public Position (int x,int y){
		x = this.x;
		y = this.y;
		wall = false;
		visited = false;
	}
	
	public void add(int dx, int dy){
		x += dx;
		y += dy;
		return;
	}
}
