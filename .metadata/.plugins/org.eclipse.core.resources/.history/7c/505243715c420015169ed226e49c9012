package maze;

public class Map {
	int [][] mapInfo; 
	int mapWidth;
	int mapHeight;
	
	public Map (int w, int h){
		mapInfo = new int[w][h];
		mapWidth 	= w+1;
		mapHeight 	= h+1;
	}
	
	public Map ( int [][]info){
		if(info== null || info.length < 1 || info[0].length < 1 ) return;
		
		mapInfo 	= info;
		mapHeight	= info.length+1;
		mapWidth 	= info[0].length+1; 
	}
}
